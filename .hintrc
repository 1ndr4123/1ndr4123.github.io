{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-align: -webkit-match-parent",
          "user-select",
          "user-select: all"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 110",
    "not and_chr <= 111",
    "not chrome <= 111",
    "not edge <= 111",
    "not firefox <= 111",
    "not safari <= 16.4",
    "not samsung <= 20",
    "not ios_saf <= 14.8"
  ]
}